<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto">
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.fragment.CashFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:layout_height="wrap_content">
    <androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/layoutParent"
        android:background="@drawable/border"
        xmlns:app="http://schemas.android.com/apk/res-auto">

        <LinearLayout
            android:id="@+id/linearLayoutMain"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/constraintLayoutDenom"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView18"
                    android:layout_width="1dp"
                    android:layout_height="0dp"
                    android:background="@color/border_color"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView21"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:textSize="13sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    android:text="Denomination"
                    app:layout_constraintEnd_toStartOf="@+id/textView18"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/linearLayoutDenom"
                    app:layout_constraintEnd_toStartOf="@+id/textView18"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView21">

                    <TextView
                        android:id="@+id/textView_2000"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:text="2000" />

                    <TextView
                        android:id="@+id/textView_500"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:text="500" />

                    <TextView
                        android:id="@+id/textView_200"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="200" />

                    <TextView
                        android:id="@+id/textView_100"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="100" />

                    <TextView
                        android:id="@+id/textView_50"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="50" />

                    <TextView
                        android:id="@+id/textView_20"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="20" />

                    <TextView
                        android:id="@+id/textView_10"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="10" />
                    <TextView
                        android:id="@+id/textView_coins"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="Coins" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/constraintLayoutTimes"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView19"
                    android:layout_width="1dp"
                    android:layout_height="0dp"
                    android:background="@color/border_color"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView22"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="center"
                    android:text="Times"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:textSize="13sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/linearLayoutTimes"
                    app:layout_constraintEnd_toStartOf="@+id/textView19"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView22">

                    <TextView
                        android:id="@+id/textView_1"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                        android:id="@+id/textView_2"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                        android:id="@+id/textView_3"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                        android:id="@+id/textView_4"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                        android:id="@+id/textView_5"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                        android:id="@+id/textView_6"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />

                    <TextView
                    android:id="@+id/textView_7"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:background="@drawable/top_border"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:textColor="@color/gray_text_color"
                    android:fontFamily="@font/poppins"
                    android:text="X" />
                    <TextView
                        android:id="@+id/textView_8"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="X" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/constraintLayoutNoOfNotes"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView181"
                    android:layout_width="1dp"
                    android:layout_height="0dp"
                    android:background="@color/border_color"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView201"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="No. of Notes"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/linearLayoutNoOfNotes"
                    app:layout_constraintEnd_toStartOf="@+id/textView181"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView201">

                    <EditText
                        android:id="@+id/textView_NoN1"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:textSize="13sp"
                        android:inputType="number"
                        android:textColorHint="@color/blue_color"

                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:hint="0" />

                    <EditText
                        android:id="@+id/textView_NoN2"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:inputType="number"
                        android:textColorHint="@color/blue_color"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:hint="0"  />

                    <EditText
                        android:id="@+id/textView_NoN3"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textColorHint="@color/blue_color"
                        android:textSize="15dp"
                        android:inputType="number"
                        android:textColor="#777"
                        android:hint="0"  />

                    <EditText
                        android:id="@+id/textView_NoN4"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:inputType="number"
                        android:textColorHint="@color/blue_color"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:hint="0"  />

                    <EditText
                        android:id="@+id/textView_NoN5"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColorHint="@color/blue_color"
                        android:inputType="number"
                        android:textColor="#777"
                        android:hint="0"  />

                    <EditText
                        android:id="@+id/textView_NoN6"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:inputType="number"
                        android:textColorHint="@color/blue_color"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:hint="0"  />

                    <EditText
                        android:id="@+id/textView_NoN7"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColorHint="@color/blue_color"
                        android:lines="1"
                        android:inputType="number"
                        android:textColor="#777"
                        android:hint="0"  />
                    <EditText
                        android:id="@+id/textView_NoN8"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="15dp"
                        android:textColorHint="@color/blue_color"
                        android:lines="1"
                        android:inputType="number"
                        android:textColor="#777"
                        android:hint="0"  />

                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:id="@+id/constraintLayoutTotalAmount"
                android:layout_height="match_parent"
                android:layout_weight="1">

                <TextView
                    android:id="@+id/textView20"
                    android:layout_width="0dp"
                    android:layout_height="42dp"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    android:text="Total Amount"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/textView25"
                    android:layout_width="0dp"
                    android:layout_height="1dp"
                    android:background="@color/border_color"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView20" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:orientation="vertical"
                    android:id="@+id/linearLayoutTotalAmount"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView20">

                    <TextView
                        android:id="@+id/textViewAmount2000"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount500"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount200"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount100"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount50"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount20"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />

                    <TextView
                        android:id="@+id/textViewAmount10"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />
                    <TextView
                        android:id="@+id/textViewAmountCoins"
                        android:layout_width="match_parent"
                        android:layout_height="35dp"
                        android:background="@drawable/top_border"
                        android:gravity="center"
                        android:textSize="13sp"
                        android:textColor="@color/gray_text_color"
                        android:fontFamily="@font/poppins"
                        android:text="0" />
                </LinearLayout>

            </androidx.constraintlayout.widget.ConstraintLayout>

        </LinearLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/linearLayoutMain">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="35dp"
                android:orientation="horizontal"
                android:weightSum="4"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView31"
                    android:layout_width="match_parent"
                    android:layout_height="35dp"
                    android:layout_weight="1"
                    android:background="@drawable/top_right"
                    android:gravity="center"
                    android:textSize="15sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    android:text="Grand Total" />

                <TextView
                    android:id="@+id/textViewGrandTotal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="3"
                    android:background="@drawable/top_border"
                    android:gravity="center"
                    android:textSize="13sp"
                    android:textColor="@android:color/black"
                    android:fontFamily="@font/poppins"
                    android:text="0" />

            </LinearLayout>
        </androidx.constraintlayout.widget.ConstraintLayout>


    </androidx.constraintlayout.widget.ConstraintLayout>
    <Button
        android:id="@+id/button_proceed"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:background="@drawable/rounded_blue_button"
        android:text="Proceed"
        android:theme="@style/ButtonTheme"
        android:layout_marginBottom="15dp"
        android:layout_marginTop="15dp"
        android:textColor="@android:color/white"
        />
    </LinearLayout>


</ScrollView>
</layout>